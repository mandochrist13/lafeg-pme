generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model institution_financiere {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  categorie        String
  contact          String
  createdAt        DateTime @db.Date
  description      String
  logo             String
  mail             String
  nom              String
  partenaire_feg   Boolean
  rs_1             String
  rs_2             String
  service          String
  site_web         String
  type_institution String
  updatedAt        DateTime @db.Date
}

model sea {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  adresse        String
  categorie      String
  contact        String
  createdAt      DateTime @db.Date
  description    String
  logo           String
  mail           String
  nom            String
  partenaire_feg Boolean
  rs_1           String
  rs_2           String?
  services       String[]
  site_web       String
  type_sea       String
  updatedAt      DateTime @db.Date
}

model texte_juridique {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  categorie      String
  createdAt      DateTime @db.Date
  date_parution  DateTime @db.Date
  description    String
  fichier_nom    String
  fichier_url    String
  mime_type      String
  taille_fichier BigInt
  titre          String
  type_texte     String
  updatedAt      DateTime @db.Date
  version        String
}

